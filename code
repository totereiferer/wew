    Private Sub btnSurr_Click(ByVal sender As System.Object, ByVal e As System.EventArgs) Handles btnSurr.Click
        bet = bet / 2
        money = money - bet
        txtMoney.Text = money
        MessageBox.Show("You surrendered. Half your bet will be deducted from your total money.")
        Call Reset(dealer, player, bet, hit)
        Call betstart(bet)
        Call Draw(dealer, player)
    End Sub

