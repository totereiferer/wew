
Public Class Blackjack
    Dim dealer(4) As Integer
    Dim player(4) As Integer
    Dim playertot, dealertot As Integer
    Dim hit As Integer
    Dim money As Integer
    Dim bet As Integer
    Dim number As Integer
    Dim value As Integer
    Dim dealercard As Integer
    Dim drawvalue1 As Integer
    Dim drawvalue2 As Integer
    Dim number1, dealercard1 As Integer
    Dim name1 As String

    Private Sub Blackjack_Load(ByVal sender As System.Object, ByVal e As System.EventArgs) Handles MyBase.Load

        name1 = InputBox("Enter your name")

        money = 10000
        hit = 1
        Call betstart(bet)
        Call Draw(dealer, player)
        txtMoney.Text = money

    End Sub
    Sub BlackjackLoad1()
        money = 10000
        hit = 1
        txtMoney.Text = money
    End Sub
    Function RNDM() As Integer
        Dim random As New Random

        number = random.Next(1, 52)

        Return number
    End Function

    Function DEALDRAW() As Integer
        Dim random As New Random
        dealercard = random.Next(1, 52)
        Select Case dealercard
            Case 1 To 4
                dealercard = 1
            Case 5 To 8
                dealercard = 2
            Case 9 To 12
                dealercard = 3
            Case 13 To 16
                dealercard = 4
            Case 17 To 20
                dealercard = 5
            Case 21 To 24
                dealercard = 6
            Case 25 To 28
                dealercard = 7
            Case 29 To 32
                dealercard = 8
            Case 33 To 36
                dealercard = 9
            Case 37 To 40
                dealercard = 10
            Case 41 To 44
                dealercard = 10
            Case 45 To 48
                dealercard = 10
            Case 49 To 52
                dealercard = 10
        End Select
        Return dealercard
    End Function
    Function RNDM1() As Integer
        Dim random As New Random

        number1 = random.Next(1, 52) * Rnd() + 1

        Return number1
    End Function

    Function DEALDRAW1() As Integer
        Dim random As New Random
        dealercard1 = random.Next(1, 52) * Rnd() + 1
        Select Case dealercard1
            Case 1 To 4
                dealercard1 = 1
            Case 5 To 8
                dealercard1 = 2
            Case 9 To 12
                dealercard1 = 3
            Case 13 To 16
                dealercard1 = 4
            Case 17 To 20
                dealercard1 = 5
            Case 21 To 24
                dealercard1 = 6
            Case 25 To 28
                dealercard1 = 7
            Case 29 To 32
                dealercard1 = 8
            Case 33 To 36
                dealercard1 = 9
            Case 37 To 40
                dealercard1 = 10
            Case 41 To 44
                dealercard1 = 10
            Case 45 To 48
                dealercard1 = 10
            Case 49 To 52
                dealercard1 = 10
        End Select
        Return dealercard1
    End Function
    Sub Reset(ByRef dealer() As Integer, ByRef player() As Integer, ByRef bet As Integer, ByRef hit As Integer)
        If money <= 0 Then
            MessageBox.Show("Game Over")
            Call BlackjackLoad1()
        Else
            ReDim player(4)
            ReDim dealer(4)
            picPlayer1.Image = Nothing
            picPlayer2.Image = Nothing
            picPlayer3.Image = Nothing
            picPlayer4.Image = Nothing
            picPlayer5.Image = Nothing
            picDealer1.Image = Nothing
            picDealer2.Image = Nothing
            picDealer3.Image = Nothing
            bet = 0
            hit = 1
        End If

    End Sub

    Sub betstart(ByRef bet As Integer)
        Try
            bet = InputBox("Enter your bet")
            txtBet.Text = bet
        Catch ex As Exception
            MessageBox.Show(ex.Message)
        End Try
    End Sub

