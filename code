    Private Sub btnStand_Click(ByVal sender As System.Object, ByVal e As System.EventArgs) Handles btnStand.Click
        dealertot = dealer.Sum
        playertot = player.Sum
        If dealertot < 17 Then
            dealer(2) = DEALDRAW()
            dealertot = dealer.Sum
            If dealertot > 21 Then
                Select Case dealer(1)
                    Case 1
                        picDealer2.Image = Image.FromFile("D:\Users\Kirt Anthony\Desktop\cards\aceclubs.jpg")
                    Case 2
                        picDealer2.Image = Image.FromFile("D:\Users\Kirt Anthony\Desktop\cards\2hearts.png")
