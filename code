        player(hit) = value
        playertot = player.Sum
        Select Case player(0)
            Case 11
                If playertot > 21 Then player(0) = 1
            Case Else
        End Select
        Select Case player(1)
            Case 11
                If playertot > 21 Then player(1) = 1
            Case Else
        End Select
        playertot = player.Sum
        Select Case True
            Case playertot <= 21
                dealertot = dealer.Sum
                playertot = player.Sum
                If dealertot < 17 Then
                    dealer(2) = DEALDRAW()
                    dealertot = dealer.Sum
                    If dealertot > 21 Then
